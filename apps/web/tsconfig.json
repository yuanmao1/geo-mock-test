{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@geo/shared-types": ["../../packages/shared-types/src/index.ts"],
      "@geo/shared-types/*": ["../../packages/shared-types/src/*"]
    }
  },
  "include": ["src", "../../packages/shared-types/src"]
}
